/*
 * ---------------------------------------------
 *  Nextflow base config file for batch_somatic
 * ---------------------------------------------
 * Imported under the default 'standard' Nextflow
 * profile in: nextflow.config
 */

params {

  //per process maxima
  max_memory = 32.GB
  max_cpus = 10
  max_time = 36.h

}

process {

  container = params.container

}
